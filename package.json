{
  "name": "@true-tech-team/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "test": "nx run-many -t test",
    "test:watch": "nx run-many -t test --watch",
    "test:coverage": "nx run-many -t test --coverage",
    "test:ci": "nx run-many -t test --configuration=ci",
    "test:ui-components": "nx test ui-components",
    "test:ui-components:watch": "nx test ui-components --watch",
    "test:ui-components:coverage": "nx test ui-components --coverage",
    "storybook": "nx storybook ui-components",
    "storybook:build": "nx build-storybook ui-components",
    "storybook:ci": "nx build-storybook ui-components --configuration=ci",
    "build": "nx run-many -t build",
    "build:production": "nx run-many -t build --configuration=production",
    "build:development": "nx run-many -t build --configuration=development",
    "build:ui-components": "nx build ui-components",
    "build:ui-components:prod": "nx build ui-components --configuration=production",
    "build:ui-components:dev": "nx build ui-components --configuration=development",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "lint:ui-components": "nx lint ui-components",
    "lint:ui-components:fix": "nx lint ui-components --fix",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ts": "tsc --noEmit",
    "lint:styles": "stylelint \"**/*.{css,scss}\"",
    "lint:styles:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "typecheck": "nx run-many -t typecheck",
    "typecheck:ui-components": "nx typecheck ui-components",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,scss,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,scss,css}\"",
    "local-registry": "nx local-registry",
    "publish:local": "nx run-many -t nx-release-publish --registry=http://localhost:4873",
    "release": "nx release",
    "release:version": "nx release version",
    "release:publish": "nx release publish",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint",
    "affected:build": "nx affected -t build",
    "graph": "nx graph",
    "reset": "nx reset"
  },
